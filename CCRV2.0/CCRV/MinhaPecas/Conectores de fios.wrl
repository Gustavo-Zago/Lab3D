Transform {
    translation 0 0 0# Alinhado verticalmente abaixo do segundo retângulo
    scale 2 0.1 0.1      # Escala para o retângulo
    children [
  Shape {
  appearance Appearance {
 
     material Material {
         diffuseColor     0.5 0.5 0.5
         emissiveColor    0 0 0
         specularColor    0 0 0
         ambientIntensity 0
         shininess        0
         transparency     0
       }
  }
     geometry Box { }
 }

]}

