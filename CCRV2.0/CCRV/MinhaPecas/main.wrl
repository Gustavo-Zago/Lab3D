#VRML V2.0 utf8

NavigationInfo {
    headlight TRUE
    type ["WALK"]
}
Viewpoint{
    position -4 2 0
    orientation 0 -1 0 1.57
} 
 
# Luzes da cena
DirectionalLight {
    direction 0 -1 0
    intensity 1
    color 1 1 1
}

PointLight {
    location 0 4 0
    intensity 0.8
    color 1 1 0.9
    radius 20
}

# Fundo da cena
Background {
    skyColor [0 0 0]
}

# Chão
Transform {
    translation 0 0 0
    children [
        Shape {
            appearance Appearance {
                material Material {
                    diffuseColor 0.5 0.5 0.5  # Cinza 
                }
                texture ImageTexture { 
                    url ["C:/Users/heler/Downloads/chao.jpg"]
                }
                textureTransform TextureTransform {
                    scale 10 10 # Repete 4 vezes no X e 4 vezes no Y
                }
            }
            geometry Box { size 12.5 0.1 15 } 
        }
    ]
}


# Teto
